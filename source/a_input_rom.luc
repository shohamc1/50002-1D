module a_input_rom (
    input address[6],  
    output a_output[16]  
  ) {
  
  const A_INPUTS = {16b1111111111001110, 16b1000000000000001, 16b0111111111111111, //add
                    16b0000000000110010,//sub
                    16b1000000000000001, 16b1000000000000001, 16b1000000000000001, 
                    16b0111111111111111, 16b0111111111111111, 16b0111111111111111,
                    16b0000000000101000, 16b0000000000101000, //bool
                    16b0000000000101000, 16b1111111111011000,  //shifter
                    16b0000000000101000, 16b1111111111011000};  //multiplier

  always {
    a_output = A_INPUTS[d34 - address];
  }
}
