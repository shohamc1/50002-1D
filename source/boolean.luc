module boolean (
    input a[16],
    input b[16],
    input io_dip[8], //alufn
    output out[16]
  ) {
  

  always {
    out = 0;
    
    // when io_dip = 24 then perform a AND b etc...
    case(io_dip[7-:8]) {
      24: out = a & b; // and
      30: out = a | b; // or
      22: out = a ^ b; // xor
      4: out = a ~^ b; // xnor
      26: out = a; // ldr
    }
  }
}
