module output_rom (
    input address[6],  
    output s_output[19]  
  ) {
  
  const OUTPUTS = {19b1000000000000000000, 19b0100000000000000010, 19b0111111111111111110, //add  
                   19b1000000000000000000, //sub
                   
                   19h00000, 19h00001, 19h00001,
                   19h00000, 19h00000, 19h00000,
                   19b0000000000001000, 19b0000000000101000, //bool
                   19b0000010100000000, 19b1111111111111110, //shifter
                   19b1111100111000000, 19b0000011001000000}; //mul

  always {
    s_output = OUTPUTS[d34 - address];
  }
}
