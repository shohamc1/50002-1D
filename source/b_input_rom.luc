module b_input_rom (
    input address[6],  
    output b_output[16]  
  ) {
  
  const B_INPUTS = {16b0000000000110010, 16b1000000000000001, 16b0111111111111111, //adder units
                    16b0000000000110010, //sub units
                    
                    16b0111111111111111, 16b0111111111111111, 16b0111111111111111,  
                    16b1000000000000001, 16b1000000000000001, 16b1000000000000001,
                    16b1111111111011000, 16b1111111111011000,
                    16b0000000000000101, 16b0000000000000101,
                    16b1111111111011000, 16b1111111111011000};

  always {
    b_output = B_INPUTS[d34 - address];
  }
}
