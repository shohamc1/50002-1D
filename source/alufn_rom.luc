module alufn_rom (
    input address[6],  
    output alufn_output[6]  
  ) {
  
  const ALUFN_INPUTS = {6d0, 6d0, 6d0, //add
                        6d1,  //sub
                        
                        6d51, 6d53, 6d55,
                        6d51, 6d53, 6d55,
                        6d24, 6d26, //bool
                        6d5, 6d7, //shifter
                        6d2, 6d2}; //mul

  always {
    alufn_output = ALUFN_INPUTS[d34 - address];
  }
}
